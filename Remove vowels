public class Vowels {
    public static void main(String[] args) {
        String input = "Programming language";
        String vowels = "aeiou";
        StringBuilder result = new StringBuilder();

        for (char ch : input.toCharArray()) {
            if (vowels.indexOf(ch)==-1) {
                result.append(ch);
            }
        }

        System.out.println(result.toString());
    }
}
