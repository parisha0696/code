import java.util.Scanner;
class sample2 {
    public static void main(String[]args){
        Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int sum=0;
        int i=0;
        while(i<=a){
            
            if(i%2==1){
              sum=sum+i;
            }
            i++;
        }
        if(sum%2==0){
            System.out.println(" is even");
        }
        else{
            System.out.println("is odd");
        }
