public class sample4 {
    public static void main(String[]args) {
        int [][]b={{4,6,8,9},{1,2,3,4},{4,6,7,3},{9,2,3,1}};
        int sum=0;
        for(int i=0;i<b.length;i++) {
            for (int j=0;j<b.length;j++) {
                sum=sum+b[i][j];
            }
        }
                System.out.print(sum);
                
                
            }
    
    
}
