import java.util.Scanner;
class sample{
      public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.println("enter the first number");
        int a=scan.nextInt();
        System.out.println("enter the second number");
        int b=scan.nextInt();
        int sum=a+b;
        System.out.println(sum);
    }
}
